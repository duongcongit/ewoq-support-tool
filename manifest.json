{
    "name": "Auto count task",
    "action": {
        "default_icon": "./icon/icon_manual.png"
    },
    "manifest_version": 3,
    "version": "1.0",
    "description": "",
    "permissions": [
        "activeTab",
        "scripting",
        "contextMenus",
        "storage",
        "tabs",
        "alarms",
        "history",
        "webNavigation"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*.googbedfsbngle.com/*"
            ],
            "js": [
                "./assets/js/jquery-3.6.1.min.js",
                "options.js"
            ]
        },
        {
            "matches": [
                "https://rating.ewoq.google.com/*"
            ],
            "js": [
                "ewoq.js"
            ]
        }
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*",
        "*://*/*",
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    }
}