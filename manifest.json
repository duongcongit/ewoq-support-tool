{
    "name": "Auto count task",
    "action": {
        "default_icon": "./icon/icon_manual.png"
    },
    "manifest_version": 3,
    "version": "1.0",
    "description": "",
    "permissions": [
        "activeTab",
        "scripting",
        "contextMenus",
        "storage",
        "tabs",
        "alarms",
        "history",
        "webNavigation"
    ],
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": [
                "https://rating.ewoq.google.com/*"
            ],
            "js": [
                "ewoq.js"
            ]
        }
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*",
        "*://*/*",
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    }
}